{
   "version": 2,
   "builds": [
       {
           "src": "/pub/index.py",
           "use": "@vercel/python"
       },
       {
           "src": "/resources/*",
           "use": "@vercel/static-build"
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "/pub/index.py"
       }
   ]
}
