{
   "version": 2,
   "builds": [
       {
           "src": "/pub/index.py",
           "use": "@vercel/python"
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "/pub/index.py"
       }
   ],
   "functions": {
     "/pub/*": {
        "maxDuration": 30
     }
   }
}
